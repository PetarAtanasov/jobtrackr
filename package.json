{
  "name": "jobtrackr",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "tw:build": "tailwindcss -i ./src/tw.css -o ./src/assets/main.css --min --content ./index.html --content ./src/**/*.vue --content ./src/**/*.js --content ./src/**/*.ts --content ./src/**/*.jsx --content ./src/**/*.tsx",
    "tw:watch": "tailwindcss -i ./src/tw.css -o ./src/assets/main.css --watch --content ./index.html --content ./src/**/*.vue --content ./src/**/*.js --content ./src/**/*.ts --content ./src/**/*.jsx --content ./src/**/*.tsx"
  },
  "dependencies": {
    "pinia": "^3.0.3",
